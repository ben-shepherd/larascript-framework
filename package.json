{
  "name": "larascript-framework",
  "version": "1.1.1",
  "description": "A Node.js framework inspired by Laravel made with TypeScript",
  "main": "index.js",
  "scripts": {
    "build": "webpack --color",
    "fix-imports": "node fix-imports.js",
    "fix-eslint": "npx eslint --fix src",
    "fix": "yarn run fix-eslint && yarn run fix-imports",
    "start": "node ./dist/app.js",
    "dev": "nodemon --watch src --ext ts --exec ts-node -r tsconfig-paths/register ./src/app.ts",
    "dev:tinker": "nodemon --watch src --ext ts --exec ts-node -r tsconfig-paths/register ./src/tinker.ts",
    "postinstall": "yarn run build",
    "test": "jest --runInBand",
    "setup": "nodemon --watch src --ext ts --exec ts-node -r tsconfig-paths/register ./src/setup.ts",
    "db:up": "cd ./docker && docker-compose -f docker-compose.network.yml -f docker-compose.mongodb.yml -f docker-compose.postgres.yml up --build -d",
    "db:down": "cd ./docker && docker-compose -f docker-compose.network.yml -f docker-compose.mongodb.yml -f docker-compose.postgres.yml down",
    "api:up": "cd ./docker && docker-compose -f docker-compose.network.yml -f docker-compose.api.yml up --build -d",
    "api:down": "cd ./docker && docker-compose -f docker-compose.network.yml -f docker-compose.api.yml down",
    "api:sh": "docker exec -it larascript-api /bin/bash"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cli-table3": "^0.6.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "glob": "^11.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.9.0",
    "pg": "^8.12.0",
    "readline": "^1.3.0",
    "sequelize": "^6.37.3",
    "sqlite3": "^5.1.7",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@jest/test-sequencer": "^29.7.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.13",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "eslint": "^9.10.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.2",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconf-checker": "^1.0.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "author": {
    "name": "Benjamin Shepherd",
    "email": "ben.shepherd@gmx.com",
    "url": "https://www.linkedin.com/in/benjamin-programmer/"
  },
  "license": "ISC"
}
