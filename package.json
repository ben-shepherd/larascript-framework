{
  "name": "larascript-framework",
  "version": "2.0.2",
  "description": "A powerful Node.js framework with built-in ORM, type safety, and developer-friendly APIs",
  "main": "index.js",
  "scripts": {
    "build": "webpack --color",
    "fix-imports": "node fix-imports.js",
    "fix-eslint": "npx eslint --fix src",
    "fix": "yarn run fix-imports && yarn run fix-eslint",
    "start": "node ./dist/app.js",
    "dev": "nodemon --watch src --ext ts --exec ts-node -r tsconfig-paths/register ./src/app.ts",
    "dev:tinker": "nodemon --watch src --ext ts --exec ts-node -r tsconfig-paths/register ./src/tinker.ts",
    "postinstall": "yarn run build",
    "test:jest": "jest --runInBand",
    "test:post": "ts-node -r tsconfig-paths/register ./src/tests/postTest.ts",
    "test": "run-s --continue-on-error test:jest test:post",
    "setup": "nodemon --watch src --ext ts --exec ts-node -r tsconfig-paths/register ./src/setup.ts",
    "network:up": "cd ./docker && docker-compose -f docker-compose.network.yml up -d network-setup",
    "db:up": "yarn db:postgres:up && yarn db:mongodb:up",
    "db:down": "yarn db:postgres:down && yarn db:mongodb:down",
    "db:restart": "yarn db:down && yarn db:up",
    "db:postgres:up": "yarn network:up && cd ./docker && docker-compose -f docker-compose.postgres.yml up --build -d postgres",
    "db:postgres:down": "cd ./docker && docker-compose -f docker-compose.postgres.yml down",
    "db:mongodb:up": "yarn network:up && cd ./docker && docker-compose -f docker-compose.mongodb.yml up --build -d mongodb",
    "db:mongodb:down": "cd ./docker && docker-compose -f docker-compose.mongodb.yml down",
    "api:up": "yarn network:up && cd ./docker && docker-compose -f docker-compose.api.yml up --build -d api",
    "api:down": "cd ./docker && docker-compose -f docker-compose.api.yml down && docker-compose -f docker-compose.network.yml down",
    "api:sh": "docker exec -it larascript-api /bin/bash"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "collect.js": "^4.36.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "glob": "^11.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongodb": "^6.9.0",
    "pg": "^8.12.0",
    "readline": "^1.3.0",
    "sequelize": "^6.37.3",
    "uuid": "^10.0.0",
    "winston": "^3.15.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@faker-js/faker": "^9.0.3",
    "@jest/test-sequencer": "^29.7.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.13",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "eslint": "^9.10.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.2",
    "npm-run-all": "^4.1.5",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconf-checker": "^1.0.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "author": {
    "name": "Benjamin Shepherd",
    "email": "ben.shepherd@gmx.com",
    "url": "https://www.linkedin.com/in/benjamin-programmer/"
  },
  "license": "ISC"
}
