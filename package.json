{
  "name": "larascript-node",
  "version": "1.0.0",
  "description": "A Node.js framework inspired by Laravel made with TypeScript",
  "main": "index.js",
  "scripts": {
    "build": "webpack --color",
    "imports": "node convert-imports.js",
    "start": "node ./dist/app.js",
    "start:debug": "node -r tsconfig-paths/register -r ./log-paths.js ./dist/app.js",
    "start:worker": "node -r tsconfig-paths/register ./dist/worker.js",
    "start:console": "node -r tsconfig-paths/register ./dist/console.js",
    "dev": "nodemon --watch src --ext ts --exec ts-node -r tsconfig-paths/register ./src/app.ts",
    "dev:debug": "nodemon --watch src --ext ts --exec ts-node -r tsconfig-paths/register ./src/debug.ts",
    "postinstall": "npm run build",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cli-table3": "^0.6.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "glob": "^11.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.7.0",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@jest/test-sequencer": "^29.7.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.13",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.2",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconf-checker": "^1.0.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "author": {
    "name": "Benjamin Shepherd",
    "email": "ben.shepherd@gmx.com",
    "url": "https://www.linkedin.com/in/benjamin-programmer/"
  },
  "license": "ISC"
}
